<?xml version="1.0"?>
 
<ehcache>
 
 <!-- default config, will applied to all entities -->
<defaultCache maxElementsInMemory="100" eternal="false" 
timeToIdleSeconds="5" timeToLiveSeconds="200" />

<!--  config, will applied to specific entities, Product here -->
<cache name="com.example.Product" maxElementsInMemory="100" 
eternal="false" timeToIdleSeconds="100" timeToLiveSeconds="200" />
 
</ehcache>
